<?xml version="1.0" encoding="utf-8"?>
<layout>

	<data />

	<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
		xmlns:app="http://schemas.android.com/apk/res-auto"
		xmlns:tools="http://schemas.android.com/tools"
		android:layout_width="match_parent"
		android:layout_height="480dp"
		tools:context="pw.janyo.janyoshare.activity.DirManagerActivity">

		<vip.mystery0.dirManager.DirManager
			android:id="@+id/dirManager"
			android:layout_width="0dp"
			android:layout_height="0dp"
			app:layout_constraintBottom_toTopOf="@id/button_ok"
			app:layout_constraintEnd_toEndOf="parent"
			app:layout_constraintStart_toStartOf="parent"
			app:layout_constraintTop_toTopOf="parent" />

		<Button
			android:id="@+id/button_ok"
			style="@style/Widget.AppCompat.Button.ButtonBar.AlertDialog"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text="@android:string/ok"
			app:layout_constraintBottom_toBottomOf="parent"
			app:layout_constraintEnd_toEndOf="parent" />

		<Button
			android:id="@+id/button_cancel"
			style="@style/Widget.AppCompat.Button.ButtonBar.AlertDialog"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text="@android:string/cancel"
			app:layout_constraintBottom_toBottomOf="@id/button_ok"
			app:layout_constraintEnd_toStartOf="@id/button_ok"
			app:layout_constraintTop_toTopOf="@id/button_ok" />

	</android.support.constraint.ConstraintLayout>
</layout>
